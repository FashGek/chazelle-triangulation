#!/bin/sh
if [ ! -f lt ]; then
        echo "lt not found; rebuilding"
        make
fi

lt g_kuratowski33   > /dev/null
lt g_kuratowski5    > /dev/null
lt g_in             > /dev/null
lt g_in2            > /dev/null
lt g_in3            > /dev/null
lt g_notquitek      > /dev/null
lt g_square         > /dev/null
lt g_triangle       > /dev/null
lt g_tie            > /dev/null 
lt g_two            > /dev/null
